riginal":{"line":40,"column":0}},{"name":"onSubmitIn","generated":{"line":53,"column":32},"source":"js/03-promises.js","original":{"line":40,"column":32}},{"generated":{"line":53,"column":42},"source":"js/03-promises.js","original":{"line":40,"column":0}}],"sources":{"js/03-promises.js":"import Notiflix from 'notiflix';\n\nconst form = document.querySelector('.form');\nconst firstDel = document.querySelector('[name=delay]');\nconst stepDel = document.querySelector('[name=step]');\nconst amountDel = document.querySelector('[name=amount]');\n\nconst createPromise = (position, delay) => {\n  return new Promise((resolve, reject) => {\n    const shouldResolve = Math.random() > 0.3;\n    setTimeout(() => {\n      if (shouldResolve) {\n        resolve({ position, delay });\n      } else {\n        reject({ position, delay });\n      }\n    }, delay);\n  });\n};\n\nconst onSubmitIn = e => {\n  e.preventDefault();\n\n  let delay = Number(firstDel.value);\n  let step = Number(stepDel.value);\n  let amount = Number(amountDel.value);\n\n  for (let i = 0; i < amount; i += 1) {\n    createPromise(i, delay)\n      .then(({ position, delay }) => {\n        Notiflix.Notify.success(`✅ Fulfilled promise ${position} in ${delay} ms`);\n      })\n      .catch(({ position, delay }) => {\n        Notiflix.Notify.failure(`❌ Rejected promise ${position} in ${delay} ms`);\n      });\n    delay += step;\n  }\n};\n\nform.addEventListener('submit', onSubmitIn);\n"},"lineCount":null}},"error":null,"hash":"6729fe7d6439c18a5e571b1196ad4e55","cacheData":{"env":{}}}